<%# My favorites %>
<% if @current_user.bookmarks.length <= 0 %>
  <h2 class= "mt-4"> Oops, you do not have any favorite yet.</h2>
<% end %>
<div class="cards-products">
  <% current_user.bookmarks.each do |bookmark| %>
    <%= render 'products/card_product', product: bookmark.product %>
  <% end %>
</div>
